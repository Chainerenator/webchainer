<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <handlers>
            <add name="httpplatformhandler" path="*" verb="*" modules="httpPlatformHandler" resourceType="Unspecified" />
        </handlers>
        <httpPlatform processPath="d:\home\site\wwwroot\journey.exe"
        arguments="-log=d:\home\site\wwwroot\log.txt -http-port=%HTTP_PLATFORM_PORT%"
        startupTimeLimit="60">
        </httpPlatform>
    </system.webServer>
</configuration>